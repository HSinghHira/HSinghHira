<hr>
</hr>
<div class="mt-2 row">
  <% if (theme.footer) { %>
  <div class="eight columns">
    <p id="madewith">
      Made with ❤️
      <% if (theme.placename && theme.placelink) { %> in
      <a href="<%- theme.placelink %>" target="_blank" rel="noreferrer"><%- theme.placename %></a>.
      <% } %>
    </p>
  </div>

  <div class="four columns posisi" style="text-align: right;">
    <div class="footer-toggle-wrapper">
      <div onclick="setDarkMode(true)" id="darkBtn" class="svg-hov">
        <%= theme.switch_dark %>
      </div>
      <div onclick="setDarkMode(false)" id="lightBtn" class="hidden svg-hov">
        <%= theme.switch_light %>
      </div>
    </div>
  </div>
  <% } %>
</div>